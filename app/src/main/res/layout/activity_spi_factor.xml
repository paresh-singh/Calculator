        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/spi"
        app:layout_constraintEnd_toStartOf="@+id/textView234"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
